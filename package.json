{
	"name": "math-helper-functions",
	"version": "1.7.4",
	"description": "Helper with misc. math functions such as sums, averages, max, min, etc",
	"main": "dist/index.js",
	"module": "dist/math-helper-functions.esm.js",
	"typings": "dist/index.d.ts",
	"files": [
		"dist",
		"src"
	],
	"engines": {
		"node": ">=10"
	},
	"author": "Alberto Rico",
	"license": "MIT",
	"repository": {
		"url": "https://github.com/alrico88/math-helper-functions"
	},
	"size-limit": [
		{
			"path": "dist/math-helper-functions.cjs.production.min.js",
			"limit": "10 KB"
		},
		{
			"path": "dist/math-helper-functions.esm.js",
			"limit": "10 KB"
		}
	],
	"keywords": [
		"math",
		"node",
		"helper",
		"sum",
		"average",
		"min",
		"max",
		"mean",
		"weighted",
		"percent",
		"domain",
		"quartile",
		"histogram"
	],
	"devDependencies": {
		"@size-limit/preset-small-lib": "^4.10.1",
		"@types/d3-array": "^2.9.0",
		"@types/lodash": "^4.14.168",
		"@typescript-eslint/eslint-plugin": "^4.17.0",
		"@typescript-eslint/parser": "^4.17.0",
		"babel-jest": "^26.6.3",
		"coveralls": "^3.1.0",
		"eslint": "^7.22.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-jest": "^24.1.0",
		"husky": "^5.1.3",
		"jest": "^26.6.1",
		"size-limit": "^4.10.1",
		"tsdx": "^0.14.1",
		"tslib": "^2.1.0",
		"typedoc": "^0.20.30",
		"typedoc-plugin-markdown": "^3.6.0",
		"typescript": "^4.2.3"
	},
	"dependencies": {
		"d3-array": "^2.0.3",
		"lodash": "^4.17.21"
	},
	"prettier": {
		"printWidth": 80,
		"semi": true,
		"singleQuote": true,
		"trailingComma": "es5"
	},
	"scripts": {
		"start": "tsdx watch",
		"build": "tsdx build",
		"test": "tsdx test",
		"lint": "tsdx lint",
		"prepare": "tsdx build",
		"size": "size-limit",
		"analyze": "size-limit --why",
		"docs": "typedoc src/index.ts --readme",
		"coveralls": "tsdx test --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
	},
	"types": "./index.d.ts"
}
